{
  "name": "rapydo",
  "version": "0.7.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 80",
    "build": "rimraf dist/* && copyfiles --flat /rapydo/courtesy/index.html dist && ng build",
    "build-aot": "rimraf dist/* && copyfiles --flat /rapydo/courtesy/index.html dist && ng build --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "old_scripts": {
    "start": "cd /rapydo && webpack-dev-server --inline --progress --colors --port 80",
    "test": "karma start",
    "build": "rimraf dist/* && copyfiles --flat /rapydo/courtesy/index.html dist && webpack --config /rapydo/config/webpack.prod.js --progress --profile --bail",
    "build-aot": "rimraf dist/* && copyfiles --flat /rapydo/courtesy/index.html dist && webpack --config /rapydo/config/webpack.prod.aot.js --progress --profile --bail"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "8.2.7",
    "@angular/compiler": "8.2.7",
    "@angular/core": "8.2.7",
    "@angular/forms": "8.2.7",
    "@angular/platform-browser": "8.2.7",
    "@angular/platform-browser-dynamic": "8.2.7",
    "@angular/router": "8.2.7",
    "@angular/animations": "8.2.7",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1",
    "bootstrap": "4.3.1",
    "@ng-bootstrap/ng-bootstrap": "4.2.1",
    "ngx-clipboard": "12.1.2",
    "@ngx-formly/core": "5.1.9",
    "@ngx-formly/bootstrap": "5.1.9",
    "@swimlane/ngx-datatable": "15.0.2",
    "moment": "2.24.0",
    "ngx-moment": "3.4.0",
    "angular-confirmation-popover": "4.2.1",
    "ng2-file-upload": "1.3.0",
    "file-saver": "2.0.2",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "@fortawesome/fontawesome-free": "5.9.0",
    "material-icons": "0.3.1",
    "jquery": "3.4.1",
    "noty": "3.2.0-beta"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
